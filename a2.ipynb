    "def read_hypercube_dataset(filename):\n",
    "    \"\"\"\n",
    "    Read the hypercube dataset from file\n",
    "    \"\"\"\n",
    "    edges = []\n",
    "    with open(filename, 'r') as f:\n",
    "        next(f)  # Skip the header line\n",
    "        for line in f:\n",
    "            parts = line.strip().split()\n",
    "            if len(parts) >= 2:\n",
    "                src = int(parts[0])\n",
    "                dst = int(parts[1])\n",
    "                edges.append((src, dst))\n",
    "    return edges\n",
